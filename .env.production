# Server Configuration\nPORT=3000\nNODE_ENV=production\nCLUSTER_MODE=true\n\n# GPS Protocol Configuration\nGPS_PORT_GT06=5023\nGPS_PORT_TELTONIKA=2023\nMAX_GPS_CONNECTIONS=2000\nGPS_CONNECTION_TIMEOUT=300000\nGPS_BATCH_SIZE=100\nGPS_BATCH_TIMEOUT=2000\n\n# Redis Configuration (Cluster Mode)\nREDIS_CLUSTER=true\nREDIS_HOST_1=localhost\nREDIS_PORT_1=6379\nREDIS_HOST_2=localhost\nREDIS_PORT_2=6380\nREDIS_HOST_3=localhost\nREDIS_PORT_3=6381\nREDIS_PASSWORD=\n\n# Redis Single Instance (if not using cluster)\n# REDIS_HOST=localhost\n# REDIS_PORT=6379\n# REDIS_DB=0\n\n# Database Configuration\nMONGO_URI=mongodb://localhost:27017/gps_tracker\nPOSTGRES_HOST=localhost\nPOSTGRES_PORT=5432\nPOSTGRES_DB=gps_tracker\nPOSTGRES_USER=postgres\nPOSTGRES_PASSWORD=password\n\n# Security\nJWT_SECRET=your-super-secret-jwt-key-change-this-in-production\nBCRYPT_ROUNDS=10\n\n# Rate Limiting\nRATE_LIMIT_WINDOW=15\nRATE_LIMIT_MAX=1000\n\n# CORS\nCORS_ORIGIN=*\n\n# Logging\nLOG_LEVEL=info\nLOG_FILE=logs/gps-tracker.log\n\n# Performance Monitoring\nMETRICS_ENABLED=true\nMETRICS_PORT=9090\n\n# Email Configuration\nSMTP_HOST=smtp.gmail.com\nSMTP_PORT=587\nSMTP_USER=your-email@gmail.com\nSMTP_PASS=your-app-password\n\n# Firebase (for push notifications)\nFIREBASE_PROJECT_ID=your-project-id\nFIREBASE_PRIVATE_KEY_ID=your-private-key-id\nFIREBASE_PRIVATE_KEY=\"-----BEGIN PRIVATE KEY-----\\nYour-Private-Key\\n-----END PRIVATE KEY-----\\n\"\nFIREBASE_CLIENT_EMAIL=firebase-adminsdk@your-project.iam.gserviceaccount.com\nFIREBASE_CLIENT_ID=your-client-id\nFIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth\nFIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token\n\n# Memory Management\nNODE_OPTIONS=\"--max-old-space-size=4096 --optimize-for-size\"\n\n# Worker Configuration\nWORKER_THREADS=4\nGPS_WORKER_POOL_SIZE=4\n\n# Circuit Breaker\nCIRCUIT_BREAKER_TIMEOUT=3000\nCIRCUIT_BREAKER_ERROR_THRESHOLD=50\nCIRCUIT_BREAKER_RESET_TIMEOUT=30000